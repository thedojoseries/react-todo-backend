{
  "name": "docker-dojo-be",
  "version": "1.0.0",
  "description": "A full stack application with React and MongoDB (Easily deployed)",
  "main": "index.js",
  "scripts": {
    "build": "webpack -p",
    "deploy": "npm run build && git commit -a -m deployment && git push heroku master",
    "dev": "webpack-dev-server --open --host 0.0.0.0 --port 8080",
    "server": "cross-env NODE_ENV=production babel-node src/server",
    "server-dev": "nodemon src/server --exec babel-node src/server",
    "start-dev": "concurrently \"npm run server-dev\" \"npm run dev\"",
    "server-spec": "babel-node src/server/server.spec",
    "start": "npm run server"
  },
  "dependencies": {
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "axios": "^0.18.0", 
    "body-parser": "^1.18.3", 
    "bootstrap": "^4.1.3", 
    "cors": "^2.8.4", 
    "css-loader": "^2.1.1", 
    "express": "^4.16.3", 
    "file-loader": "^3.0.1",
    "history": "^4.7.2", 
    "md5": "^2.2.1", 
    "mongodb": "^3.1.8", 
    "node-sass": "^4.11.0",
    "react": "^16.4.2",
    "react-dom": "^16.5.0", 
    "react-redux": "^5.0.7", 
    "react-router": "^4.3.1", 
    "react-router-dom": "^4.3.1", 
    "redux": "^4.0.0", 
    "redux-logger": "^3.0.6",
    "redux-saga": "^0.16.2",
    "sass-loader": "^7.1.0",
    "url-loader": "^1.1.2",
    "uuid": "^3.3.2",
    "webpack": "^4.17.2"
  },
  "engines": {
    "node": "9.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/node": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.0.0",
    "babel-loader": "^8.0.2",
    "concurrently": "^4.0.1",
    "cross-env": "^5.2.0",
    "highground": "^1.0.3-beta",
    "jest": "^23.6.0",
    "mongodb": "^3.1.10",
    "nodemon": "^1.18.4",
    "style-loader": "^0.23.0",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.7"
  }
}
